<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="tts.project.handi.activity.fragment.OrdersFragment">


    <android.support.design.widget.TabLayout
        android:id="@+id/tablayout_order"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        app:tabIndicatorColor="@color/RGB34_176_222"
        app:tabSelectedTextColor="@color/RGB34_176_222">

    </android.support.design.widget.TabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager_order"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v4.view.ViewPager>

</LinearLayout>
