<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y145"
        android:background="@color/RGB255_255_255"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/kjsdjkdsjkd"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/x20"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/my_message_phone"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="0.8"
                android:gravity="center_vertical"
                android:text="13309248019"
                android:textSize="@dimen/y30" />

            <TextView
                android:id="@+id/my_message_province"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="0.5" />

            <TextView
                android:id="@+id/my_message_detail"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/y20"

                android:layout_weight="0.5"
                android:text="长清路507号恒大长青坊" />
        </LinearLayout>

        <TextView
            android:id="@+id/my_message_default"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x20"
            android:text="默认地址"
            android:textColor="#000000"
            android:textSize="@dimen/y30" />

    </RelativeLayout>
</LinearLayout>
